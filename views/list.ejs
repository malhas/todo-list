<%- include("header")  -%>
<body>
    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    </div>
    <div class="box" id="item">
            <% for( let index = 0; index < list.length; index++ ) { %>
            <div class="item">
                <input type="checkbox"><p><%= list[index].name %> </p>
            </div>
            <% } %>
        <form class="item" action="/" method="post">
            <input type="text" name="newItem" placeholder="New Item"> <button type="submit" value=<%= listTitle %> >+</button>
        </form>
    </div>
</body>
<%- include("footer")  -%>